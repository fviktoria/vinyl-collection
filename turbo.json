{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    }
  },
  "globalEnv": ["NEXT_PUBLIC_OWNER_NAME","NEXT_PUBLIC_OWNER_AVATAR", "NEXT_PUBLIC_OWNER_HANDLE","DISCOGS_TOKEN","DISCOGS_USER_AGENT","DISCOGS_USER_NAME","UPSTASH_URL","UPSTASH_TOKEN"]
}
